{
  "arguments": [
    {
      "default_value": "",
      "description": "Top-level namespace",
      "name": "namespace"
    },
    {
      "default_value": "False",
      "description": "Whether run a SLAM",
      "name": "slam"
    },
    {
      "default_value": "",
      "description": "Full path to map yaml file to load",
      "name": "map"
    },
    {
      "default_value": "",
      "description": "Full path to keepout mask yaml file to load",
      "name": "keepout_mask"
    },
    {
      "default_value": "",
      "description": "Full path to speed mask yaml file to load",
      "name": "speed_mask"
    },
    {
      "default_value": "",
      "description": "Path to the graph file to load",
      "name": "graph"
    },
    {
      "default_value": "false",
      "description": "Use simulation (Gazebo) clock if true",
      "name": "use_sim_time"
    },
    {
      "default_value": "${os.path.join:['${get_package_share_directory:nav2_bringup}', 'params', 'nav2_params.yaml']}",
      "description": "Full path to the ROS2 parameters file to use for all launched nodes",
      "name": "params_file"
    },
    {
      "default_value": "true",
      "description": "Automatically startup the nav2 stack",
      "name": "autostart"
    },
    {
      "default_value": "True",
      "description": "Whether to use composed bringup",
      "name": "use_composition"
    },
    {
      "default_value": "False",
      "description": "Whether to respawn if a node crashes. Applied when composition is disabled.",
      "name": "use_respawn"
    },
    {
      "default_value": "info",
      "description": "log level",
      "name": "log_level"
    },
    {
      "default_value": "True",
      "description": "Whether to enable localization or not",
      "name": "use_localization"
    },
    {
      "default_value": "True",
      "description": "Whether to enable keepout zones or not",
      "name": "use_keepout_zones"
    },
    {
      "default_value": "True",
      "description": "Whether to enable speed zones or not",
      "name": "use_speed_zones"
    }
  ],
  "groups": [
    {
      "actions": {
        "nodes": [
          {
            "condition": "${IfCondition:${LaunchConfiguration:use_composition}}",
            "name": "nav2_container",
            "namespace": "${LaunchConfiguration:namespace}",
            "package": "rclcpp_components",
            "executable": "component_container_isolated",
            "parameters": [
              "${ParameterFile:${CustomHandler:RewrittenYaml(source_file=${LaunchConfiguration:params_file}, root_key=${LaunchConfiguration:namespace}, param_rewrites={}, value_rewrites={'KEEPOUT_ZONE_ENABLED': '${LaunchConfiguration:use_keepout_zones}', 'SPEED_ZONE_ENABLED': '${LaunchConfiguration:use_speed_zones}'}, convert_types=True)} allow_substs=True}",
              {
                "autostart": "${LaunchConfiguration:autostart}"
              }
            ],
            "arguments": [
              "--ros-args",
              "--log-level",
              "${LaunchConfiguration:log_level}"
            ],
            "remappings": [
              [
                "/tf",
                "tf"
              ],
              [
                "/tf_static",
                "tf_static"
              ]
            ],
            "output": "screen"
          }
        ],
        "includes": [
          {
            "launch_description_source": "${os.path.join:[\"${os.path.join:['${get_package_share_directory:nav2_bringup}', 'launch']}\", 'slam_launch.py']}",
            "launch_arguments": {
              "namespace": "${LaunchConfiguration:namespace}",
              "use_sim_time": "${LaunchConfiguration:use_sim_time}",
              "autostart": "${LaunchConfiguration:autostart}",
              "use_respawn": "${LaunchConfiguration:use_respawn}",
              "params_file": "${LaunchConfiguration:params_file}"
            },
            "included": {},
            "condition": "${IfCondition:${LaunchConfiguration:slam} and ${LaunchConfiguration:use_localization}}"
          },
          {
            "launch_description_source": "${os.path.join:[\"${os.path.join:['${get_package_share_directory:nav2_bringup}', 'launch']}\", 'localization_launch.py']}",
            "launch_arguments": {
              "namespace": "${LaunchConfiguration:namespace}",
              "map": "${LaunchConfiguration:map}",
              "use_sim_time": "${LaunchConfiguration:use_sim_time}",
              "autostart": "${LaunchConfiguration:autostart}",
              "params_file": "${LaunchConfiguration:params_file}",
              "use_composition": "${LaunchConfiguration:use_composition}",
              "use_respawn": "${LaunchConfiguration:use_respawn}",
              "container_name": "nav2_container"
            },
            "included": {},
            "condition": "${IfCondition:not ${LaunchConfiguration:slam} and ${LaunchConfiguration:use_localization}}"
          },
          {
            "launch_description_source": "${os.path.join:[\"${os.path.join:['${get_package_share_directory:nav2_bringup}', 'launch']}\", 'keepout_zone_launch.py']}",
            "launch_arguments": {
              "namespace": "${LaunchConfiguration:namespace}",
              "keepout_mask": "${LaunchConfiguration:keepout_mask}",
              "use_sim_time": "${LaunchConfiguration:use_sim_time}",
              "params_file": "${LaunchConfiguration:params_file}",
              "use_composition": "${LaunchConfiguration:use_composition}",
              "use_respawn": "${LaunchConfiguration:use_respawn}",
              "container_name": "nav2_container"
            },
            "included": {},
            "condition": "${IfCondition:${LaunchConfiguration:use_keepout_zones}}"
          },
          {
            "launch_description_source": "${os.path.join:[\"${os.path.join:['${get_package_share_directory:nav2_bringup}', 'launch']}\", 'speed_zone_launch.py']}",
            "launch_arguments": {
              "namespace": "${LaunchConfiguration:namespace}",
              "speed_mask": "${LaunchConfiguration:speed_mask}",
              "use_sim_time": "${LaunchConfiguration:use_sim_time}",
              "params_file": "${LaunchConfiguration:params_file}",
              "use_composition": "${LaunchConfiguration:use_composition}",
              "use_respawn": "${LaunchConfiguration:use_respawn}",
              "container_name": "nav2_container"
            },
            "included": {},
            "condition": "${IfCondition:${LaunchConfiguration:use_speed_zones}}"
          },
          {
            "launch_description_source": "${os.path.join:[\"${os.path.join:['${get_package_share_directory:nav2_bringup}', 'launch']}\", 'navigation_launch.py']}",
            "launch_arguments": {
              "namespace": "${LaunchConfiguration:namespace}",
              "use_sim_time": "${LaunchConfiguration:use_sim_time}",
              "autostart": "${LaunchConfiguration:autostart}",
              "graph": "${LaunchConfiguration:graph}",
              "params_file": "${LaunchConfiguration:params_file}",
              "use_composition": "${LaunchConfiguration:use_composition}",
              "use_respawn": "${LaunchConfiguration:use_respawn}",
              "use_keepout_zones": "${LaunchConfiguration:use_keepout_zones}",
              "use_speed_zones": "${LaunchConfiguration:use_speed_zones}",
              "container_name": "nav2_container"
            },
            "included": {}
          }
        ]
      }
    }
  ],
