{
    "name": "launchmap",
    "displayName": "LaunchMap",
    "description": "Visualize ROS2 Launch Files",
    "publisher": "kodorobotics",
    "icon": "assets/launchmap-logo.png",
    "repository": {
        "type": "git",
        "url": "https://github.com/Kodo-Robotics/launchmap"
    },
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.70.0"
    },
    "main": "./dist/extension.js",
    "activationEvents": [
        "onCommand:launchmap.openVisualizer"
    ],
    "contributes": {
        "commands": [
            {
                "command": "launchmap.openVisualizer",
                "title": "Open Launch Visualizer"
            }
        ]
    },
    "scripts": {
        "compile": "tsc"
    },
    "devDependencies": {
        "@types/node": "^24.0.1",
        "@types/vscode": "^1.70.0",
        "@types/which": "^3.0.4",
        "typescript": "^4.7.4"
    },
    "dependencies": {
        "which": "^5.0.0"
    }
}
